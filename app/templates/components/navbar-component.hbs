<div class="sidebar">

  {{#each content as |content|}}

    {{#link-to content.link}}
      <div class="nav-content">
        <div class="icon nav-content__icon">
          {{material-icon icon=content.icon}}
        </div>
        <div class="nav-content__text">
          {{content.text}}
        </div>

      </div>
    {{/link-to}}
  {{/each}}
</div>